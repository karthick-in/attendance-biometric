<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.attendance"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />
	
    <application android:label="@string/app_name" android:icon="@drawable/att">
   
        <activity android:name=".Loginact"
                  android:label="Attendance application">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
		<activity android:name=".Viewattendance"
                  android:label="Attendance application">
            <intent-filter>
                <action android:name="android.intent.action.viewattendance" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Resetpassword"
                  android:label="Attendance application">
            <intent-filter>
                <action android:name="android.intent.action.resetpassword" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".registrationact"
                  android:label="Attendance application">
            <intent-filter>
                <action android:name="android.intent.action.registration" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
    </application>
</manifest>